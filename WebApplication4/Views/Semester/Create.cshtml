@model WebApplication4.Models.Semester
@{
    ViewData["Title"] = "Create";
}



<form asp-action="Create" method="post">

    <div class="row">


        <div class="col-sm-3">
        </div>
        <div class="col-sm-5">
            <h3>CREATE Semister</h3>

           

            <div class="form-group">
                <label class="control-label">Semister Name</label>
                <input asp-for="SemesterName" class="form-control" />
            </div>

            <div class="form-group">
                <label class="control-label">Semister Number</label>
                <input asp-for="SemesterNo" class="form-control" />
            </div>

           
            <br />
            <div class="form-group float-right">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </div>
        <div class="col-sm-3">
        </div>
    </div>

</form>
